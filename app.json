{
  "expo": {
    "name": "EDUCAFRIC",
    "slug": "educafric",
    "owner": "broximo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/android-icon-192x192.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/splash-icon-512x512.png",
      "resizeMode": "contain",
      "backgroundColor": "#7C5CFC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.educafric.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/android-icon-192x192.png",
        "backgroundColor": "#7C5CFC"
      },
      "package": "com.educafric.app",
      "versionCode": 2,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./public/favicon.ico",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "12e440d5-ec8e-40d0-9185-38e0f42be9ff"
      }
    }
  }
}
